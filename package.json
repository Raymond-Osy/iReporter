{
  "name": "ireporter",
  "version": "1.0.0",
  "description": "An app where everyone can make a report",
  "main": "index.js",
  "scripts": {
    "build": "babel server -d dist/server",
    "start": "nodemon --exec babel-node server/app.js",
    "start:dev": "nodemon --exec babel-node server/app.js",
    "test": "mocha server/test/*.js --exit  --require babel-register --reporter spec",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coverage": "nyc report  --reporter=html  --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raymond-Osy/iReporter.git"
  },
  "author": "Raymond Akalonu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Raymond-Osy/iReporter/issues"
  },
  "homepage": "https://github.com/Raymond-Osy/iReporter#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "express": "^4.16.4",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.7",
    "nyc": "^13.1.0"
  }
}
